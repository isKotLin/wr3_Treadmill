<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:background="@mipmap/dialog_bj">

    <!--<ImageView-->
        <!--android:id="@+id/version_iv_close"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:layout_alignParentRight="true"-->
        <!--android:padding="@dimen/my10dp"-->
        <!--android:src="@mipmap/close" />-->

    <ImageView
        android:id="@+id/version_iv_sign"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/my20dp"
        android:src="@mipmap/versions_icon" />

    <TextView
        android:id="@+id/dialog_tv_version"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/version_iv_sign"
        android:layout_marginLeft="@dimen/my80dp"
        android:layout_marginRight="@dimen/my80dp"
        android:layout_marginTop="@dimen/my10dp"
        android:textColor="@android:color/white"
        android:textSize="@dimen/my24dp" />

    <TextView
        android:id="@+id/dialog_tv_music_version"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/dialog_tv_version"
        android:layout_below="@id/dialog_tv_version"
        android:layout_marginTop="@dimen/my10dp"
        android:layout_marginRight="@dimen/my80dp"
        android:textColor="@android:color/white"
        android:textSize="@dimen/my24dp" />

    <TextView
        android:id="@+id/dialog_tv_video_version"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/dialog_tv_music_version"
        android:layout_below="@id/dialog_tv_music_version"
        android:layout_marginTop="@dimen/my10dp"
        android:layout_marginRight="@dimen/my80dp"
        android:textColor="@android:color/white"
        android:textSize="@dimen/my24dp" />

    <TextView
        android:id="@+id/dialog_tv_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/dialog_tv_video_version"
        android:layout_below="@id/dialog_tv_video_version"
        android:layout_marginTop="@dimen/my10dp"
        android:layout_marginRight="@dimen/my80dp"
        android:textColor="@android:color/white"
        android:textSize="@dimen/my24dp" />

    <TextView
        android:id="@+id/dialog_tv_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/dialog_tv_version"
        android:layout_below="@id/dialog_tv_date"
        android:layout_marginTop="@dimen/my10dp"
        android:layout_marginRight="@dimen/my80dp"
        android:layout_marginBottom="@dimen/my10dp"
        android:textColor="@android:color/white"
        android:textSize="@dimen/my24dp" />
</RelativeLayout>